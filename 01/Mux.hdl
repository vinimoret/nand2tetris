/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel , out=notSel );
    And(a= sel , b= b , out= out2 );
    And(a=notSel , b= a , out= out1 );
    Xor(a =out1 , b = out2, out = out);
    //// Replace this comment with your code.
}