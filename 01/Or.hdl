/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a , out=out1 );
    Not(in=b , out= out2);
    Nand(a= out1 , b= out2 , out= out );
}